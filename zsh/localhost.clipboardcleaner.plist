<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
                       "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>Label</key>
    <string>localhost.clipboardcleaner</string>
    <key>ProgramArguments</key>
    <array>
      <string>/Users/shi/.clipboard_cleaner.sh</string>
    </array>
    <key>RunAtLoad</key>
    <true/>
    <key>KeepAlive</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/tmp/clipboard_cleaner.log</string>
    <key>StandardErrorPath</key>
    <string>/tmp/clipboard_cleaner_err.log</string>
  </dict>
</plist>
